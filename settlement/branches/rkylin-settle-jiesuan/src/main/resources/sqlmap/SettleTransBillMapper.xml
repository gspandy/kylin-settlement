  <!-- 关联查询 trans_loan 查询待结算的分润结果 -->
  <select id="queryloanByCreatedTime" parameterType="map" resultMap="BaseResultMap">
	SELECT
	    P.TRANS_BILL_ID,
	    P.ORDER_NO,
	    P.BATCH_NO,
	    P.BILL_NO,
	    P.ROOT_INST_CD,
	    P.PRODUCT_ID,
	    P.USER_ID,
	    P.ROLE_CODE,
	    P.INTER_USER_ID,
	    P.INTER_ROLE_CODE,
	    P.INTO_PRODUCT_ID,
	    P.REFER_USER_ID,
	    P.REFER_ROLE_CODE,
	    P.REFER_PRODUCT_ID,
	    P.REFER_USER_ID2,
	    P.REFER_ROLE_CODE2,
	    P.SETTLE_OBJECT,
	    P.SETTLE_TYPE,
	    P.REFER_PRODUCT_ID2,
	    P.BILL_AMOUNT,
	    P.BILL_TYPE,
	    P.OBLIGATE1,
	    P.OBLIGATE2,
	    P.STATUS_ID,
	    P.REMARK,
	    P.ACCOUNT_DATE,
	    P.CREATED_TIME,
	    P.UPDATED_TIME
	FROM
		SETTLE_TRANS_BILL P
	JOIN SETTLE_LOAN_DETAIL D ON (P.ORDER_NO = D.LOAN_ID AND P.ROOT_INST_CD = D.ROOT_INST_CD)
	WHERE
		P.STATUS_ID = ${proStatusId} 
	AND 
		P.BILL_TYPE = #{proBillType} 
	AND 
		DATE_FORMAT(P.ACCOUNT_DATE, '%Y-%m-%d') &lt;= DATE_FORMAT(#{settleTime, jdbcType=TIMESTAMP}, '%Y-%m-%d')
	AND	
		D.STATUS_ID IN 
